{
    "name": "hyperf/payment",
    "type": "library",
    "description": "支付宝支付、微信支付、招商一网通支付php SDK。方便快速接入，最完整的开源支付 php sdk",
    "keywords": ["alipay", "weixin", "支付宝支付", "微信支付", "集成支付接口SDK", "招商一网通", "一网通"],
    "homepage": "http://helei112g.github.io/payment",
    "license": "MIT",
    "authors": [
        {
            "name": "mytty",
            "email": "1297942619@qq.com"
        },
        {
            "name": "Leo",
            "email": "dayugog@gmail.com",
            "homepage": "https://dayutalk.cn"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "ext-openssl": "*",
        "hyperf/guzzle": "~2.1.0",
        "hyperf/logger": "~2.1.0",
        "hyperf/event": "~2.1.0"
    },
    "require-dev": {
        "codeception/codeception": "*"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-mbstring": "Required to use mbstring.",
        "ext-bcmath": "Required to use bcmath.",
        "ext-simplexml": "Required to use simplexml.",
        "ext-xml": "Required to use xml."
    },
    "autoload": {
        "psr-4": {
            "myttyy\\Hyperf\\Payment\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "myttyy\\Hyperf\\Payment\\ConfigProvider"
        }
    }
}
